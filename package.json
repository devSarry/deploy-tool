{
  "name": "deployer",
  "version": "0.0.1",
  "description": "A tool that will ssh into a server pull in the app files and run any scripts and set up nginx and git hooks",
  "scripts": {
    "test": "./node_modules/.bin/babel-tape-runner ./spec/**/*.spec.js | ./node_modules/.bin/tap-spec",
    "zuul": "./node_modules/.bin/zuul --local --open -- spec/**/*.spec.js",
    "build": "./node_modules/.bin/babel -d ./dist ./src",
    "publish": "git push && git push --tags && npm publish"
  },
  "author": "Jonathan Sarry",
  "license": "MIT",
  "bin": {
    "deployer": "dist/index.js"
  },
  "dependencies": {
    "babel-cli": "^6.22.0",
    "babel-eslint": "*",
    "babel-preset-es2015": "*",
    "babel-tape-runner": "*",
    "babelify": "7.2.0",
    "chalk": "^1.1.3",
    "clear": "0.0.1",
    "clui": "^0.3.1",
    "commander": "^2.9.0",
    "eslint": "*",
    "eslint-config-airbnb": "*",
    "figlet": "^1.1.2",
    "github": "^2.1.0",
    "inquirer": "^1.1.0",
    "lodash": "^4.13.1",
    "log": "^1.4.0",
    "minimist": "^1.2.0",
    "node-readme": "^0.1.8",
    "node-ssh": "^4.0.1",
    "preferences": "^0.2.1",
    "simple-git": "^1.40.0",
    "tap-spec": "^4.0.2",
    "tape": "^4.0.0",
    "touch": "^1.0.0",
    "zuul": "^3.8.0"
  },
  "files": [
    "dist/*",
    "README.md",
    "LICENCE"
  ]
}
